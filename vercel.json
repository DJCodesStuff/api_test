{
    "version": 2,
    "builds": [
      {
        "src": "api/submit_form.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/api/submit_form",
        "dest": "/api/submit_form.py"
      },
      {
        "src": "/(.*)",
        "dest": "/public/$1"
      }
    ]
  }
  